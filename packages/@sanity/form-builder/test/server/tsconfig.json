{
  "extends": "../../tsconfig",
  "compilerOptions": {
    "rootDir": "../..",
    "jsx": "react",
    "baseUrl": "../..",
    "paths": {
      "@sanity/base": ["../base/src"],
      "@sanity/base/*": ["../base/src/*"],
      "@sanity/form-builder/*": ["./src/*"],
